@model IEnumerable<Tp_TransportesRaffi.Models.Viaje>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaHoraEntrega)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DomicilioEntrega)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescripcionPaquete)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorViaje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoViaje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdclienteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdvehiculoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaHoraEntrega)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DomicilioEntrega)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescripcionPaquete)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorViaje)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoViaje)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdclienteNavigation.Cuit)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdvehiculoNavigation.Patente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
